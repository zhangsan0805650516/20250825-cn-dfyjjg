    <sql id="selectFaMemberImageVo">
        select id, member_id, avatar, id_card_front_image, id_card_back_image, card_image, avatar_url, id_card_front_image_url, id_card_back_image_url, card_image_url, status, create_time, create_by, update_time, update_by, remarks, delete_flag from fa_member_image
