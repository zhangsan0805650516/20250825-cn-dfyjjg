            <if test="remarks != null  and remarks != ''"> and remarks = #{remarks}</if>
            <if test="deleteFlag != null "> and delete_flag = #{deleteFlag}</if>
        </where>
    </select>
    
    <select id="selectFaMemberImageById" parameterType="Integer" resultMap="FaMemberImageResult">
        <include refid="selectFaMemberImageVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertFaMemberImage" parameterType="FaMemberImage" useGeneratedKeys="true" keyProperty="id">
        insert into fa_member_image
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="memberId != null">member_id,</if>
            <if test="avatar != null">avatar,</if>
            <if test="idCardFrontImage != null">id_card_front_image,</if>
            <if test="idCardBackImage != null">id_card_back_image,</if>
