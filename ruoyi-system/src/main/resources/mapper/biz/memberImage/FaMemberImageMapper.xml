            <if test="idCardFrontImageUrl != null  and idCardFrontImageUrl != ''"> and id_card_front_image_url = #{idCardFrontImageUrl}</if>
            <if test="idCardBackImageUrl != null  and idCardBackImageUrl != ''"> and id_card_back_image_url = #{idCardBackImageUrl}</if>
            <if test="cardImageUrl != null  and cardImageUrl != ''"> and card_image_url = #{cardImageUrl}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="createTime != null "> and create_time = #{createTime}</if>
