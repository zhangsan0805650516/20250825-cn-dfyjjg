    </update>

    <delete id="deleteFaMemberImageById" parameterType="Integer">
        update fa_member_image set delete_flag = 1 where id = #{id}
    </delete>

    <delete id="deleteFaMemberImageByIds" parameterType="String">
        update fa_member_image set delete_flag = 1 where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>