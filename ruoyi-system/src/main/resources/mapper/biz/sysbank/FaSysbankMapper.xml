            <if test="paymentGateId != null">#{paymentGateId},</if>
            <if test="paymentAction != null">#{paymentAction},</if>
            <if test="paymentCallbackUrl != null">#{paymentCallbackUrl},</if>
         </trim>
    </insert>

    <update id="updateFaSysbank" parameterType="FaSysbank">
        update fa_sysbank
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null">name = #{name},</if>
            <if test="bankInfo != null">bank_info = #{bankInfo},</if>
            <if test="account != null">account = #{account},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="tdName != null">td_name = #{tdName},</if>
            <if test="tdAlias != null">td_alias = #{tdAlias},</if>
            <if test="khzhihang != null">khzhihang = #{khzhihang},</if>
            <if test="adminId != null">admin_id = #{adminId},</if>
            <if test="ckPass != null">ck_pass = #{ckPass},</if>
            <if test="deleteFlag != null">delete_flag = #{deleteFlag},</if>
            <if test="paymentType != null">payment_type = #{paymentType},</if>
            <if test="paymentGateway != null">payment_gateway = #{paymentGateway},</if>
            <if test="paymentAppId != null">payment_app_id = #{paymentAppId},</if>
            <if test="paymentApiKey != null">payment_api_key = #{paymentApiKey},</if>
            <if test="paymentMemberId != null">payment_member_id = #{paymentMemberId},</if>
            <if test="paymentMd5Key != null">payment_md5_key = #{paymentMd5Key},</if>
