        <result property="paymentMd5Key"    column="payment_md5_key"    />
        <result property="paymentBankCode"    column="payment_bank_code"    />
        <result property="paymentTypId"    column="payment_typ_id"    />
        <result property="paymentGateId"    column="payment_gate_id"    />
        <result property="paymentAction"    column="payment_action"    />
        <result property="paymentCallbackUrl"    column="payment_callback_url"    />
    </resultMap>

    <sql id="selectFaSysbankVo">
        select
            id,
            name,
            bank_info,
            account,
            status,
            create_time,
            update_time,
            td_name,
            td_alias,
            khzhihang,
            admin_id,
            ck_pass,
            delete_flag,
            payment_type,
            payment_gateway,
            payment_app_id,
            payment_api_key,
            payment_member_id,
            payment_md5_key,
            payment_bank_code,
            payment_typ_id,
            payment_gate_id,
            payment_action,
            payment_callback_url
        from fa_sysbank
    </sql>

    <select id="selectFaSysbankList" parameterType="FaSysbank" resultMap="FaSysbankResult">
        <include refid="selectFaSysbankVo"/>
        <where>  
            <if test="id != null "> and id = #{id}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="bankInfo != null  and bankInfo != ''"> and bank_info = #{bankInfo}</if>
            <if test="account != null  and account != ''"> and account = #{account}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="createTime != null "> and create_time = #{createTime}</if>
            <if test="updateTime != null "> and update_time = #{updateTime}</if>
            <if test="tdName != null  and tdName != ''"> and td_name like concat('%', #{tdName}, '%')</if>
            <if test="tdAlias != null  and tdAlias != ''"> and td_alias like concat('%', #{tdAlias}, '%')</if>
            <if test="khzhihang != null  and khzhihang != ''"> and khzhihang = #{khzhihang}</if>
            <if test="adminId != null "> and admin_id = #{adminId}</if>
            <if test="ckPass != null  and ckPass != ''"> and ck_pass = #{ckPass}</if>
            <if test="deleteFlag != null "> and delete_flag = #{deleteFlag}</if>
            <if test="paymentType != null "> and payment_type = #{paymentType}</if>
            <if test="paymentGateway != null "> and payment_gateway = #{paymentGateway}</if>
            <if test="paymentAppId != null "> and payment_app_id = #{paymentAppId}</if>
            <if test="paymentApiKey != null "> and payment_api_key = #{paymentApiKey}</if>
            <if test="paymentMemberId != null "> and payment_member_id = #{paymentMemberId}</if>
            <if test="paymentMd5Key != null "> and payment_md5_key = #{paymentMd5Key}</if>
            <if test="paymentBankCode != null "> and payment_bank_code = #{paymentBankCode}</if>
            <if test="paymentTypId != null "> and payment_typ_id = #{paymentTypId}</if>
            <if test="paymentGateId != null "> and payment_gate_id = #{paymentGateId}</if>
            <if test="paymentAction != null "> and payment_action = #{paymentAction}</if>
            <if test="paymentCallbackUrl != null "> and payment_callback_url = #{paymentCallbackUrl}</if>
        </where>
    </select>
    
    <select id="selectFaSysbankById" parameterType="Long" resultMap="FaSysbankResult">
        <include refid="selectFaSysbankVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertFaSysbank" parameterType="FaSysbank" useGeneratedKeys="true" keyProperty="id">
        insert into fa_sysbank
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null">name,</if>
            <if test="bankInfo != null">bank_info,</if>
            <if test="account != null">account,</if>
            <if test="status != null">status,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="tdName != null">td_name,</if>
            <if test="tdAlias != null">td_alias,</if>
            <if test="khzhihang != null">khzhihang,</if>
            <if test="adminId != null">admin_id,</if>
            <if test="ckPass != null">ck_pass,</if>
            <if test="deleteFlag != null">delete_flag,</if>
            <if test="paymentType != null">payment_type,</if>
            <if test="paymentGateway != null">payment_gateway,</if>
            <if test="paymentAppId != null">payment_app_id,</if>
            <if test="paymentApiKey != null">payment_api_key,</if>
            <if test="paymentMemberId != null">payment_member_id,</if>
            <if test="paymentMd5Key != null">payment_md5_key,</if>
            <if test="paymentBankCode != null">payment_bank_code,</if>
            <if test="paymentTypId != null">payment_typ_id,</if>
            <if test="paymentGateId != null">payment_gate_id,</if>
            <if test="paymentAction != null">payment_action,</if>
            <if test="paymentCallbackUrl != null">payment_callback_url,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null">#{name},</if>
            <if test="bankInfo != null">#{bankInfo},</if>
            <if test="account != null">#{account},</if>
            <if test="status != null">#{status},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="tdName != null">#{tdName},</if>
            <if test="tdAlias != null">#{tdAlias},</if>
            <if test="khzhihang != null">#{khzhihang},</if>
            <if test="adminId != null">#{adminId},</if>
            <if test="ckPass != null">#{ckPass},</if>
            <if test="deleteFlag != null">#{deleteFlag},</if>
            <if test="paymentType != null">#{paymentType},</if>
            <if test="paymentGateway != null">#{paymentGateway},</if>
            <if test="paymentAppId != null">#{paymentAppId},</if>
            <if test="paymentApiKey != null">#{paymentApiKey},</if>
            <if test="paymentMemberId != null">#{paymentMemberId},</if>
            <if test="paymentMd5Key != null">#{paymentMd5Key},</if>
            <if test="paymentBankCode != null">#{paymentBankCode},</if>
